{
	"extends": ["stylelint-config-standard", "stylelint-config-recess-order"],
	"rules": {
		"property-no-unknown": [
			true,
			{
				"ignoreProperties": ["composes", "compose-with"]
			}
		],
		"at-rule-no-unknown": [
			true,
			{
				"ignoreAtRules": ["tailwind", "define-mixin", "mixin"]
			}
		]
	}
}
